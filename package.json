{
  "name": "@mx-space/server",
  "version": "1.0.0-alpha.3",
  "description": "Mix Space Server",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mx-space/server.git"
  },
  "author": "Innei",
  "bugs": {
    "url": "https://github.com/mx-space/server/issues"
  },
  "files": [
    "dist/src/*",
    "dist/libs/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "homepage": "https://github.com/mx-space/server#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start -w | pino-colada",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "prod": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js",
    "prod:pm2": "cross-env NODE_ENV=production pm2 restart ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:debug": "cross-env NODE_ENV=production nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --cache --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.3.2",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.3.2",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-fastify": "7.3.2",
    "@nestjs/platform-socket.io": "7.3.2",
    "@nestjs/schedule": "0.4.0",
    "@nestjs/serve-static": "2.1.3",
    "@nestjs/swagger": "4.5.12",
    "@nestjs/websockets": "7.3.2",
    "@typegoose/auto-increment": "0.5.0",
    "@typegoose/typegoose": "7.3.0",
    "akismet": "2.0.6",
    "bcrypt": "5.0.0",
    "cache-manager": "3.3.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "cos-nodejs-sdk-v5": "2.6.2",
    "dayjs": "1.8.30",
    "ejs": "3.1.3",
    "fastify": "2.15.2",
    "fastify-cookie": "3.6.1",
    "fastify-helmet": "3.0.2",
    "fastify-multipart": "1.0.6",
    "fastify-session": "4.0.1",
    "fastify-swagger": "2.6.0",
    "file-type": "14.6.2",
    "get-folder-size": "2.0.1",
    "image-size": "0.8.3",
    "mongoose": "5.9.25",
    "mongoose-lean-virtuals": "0.6.8",
    "mongoose-unique-validator": "2.0.3",
    "nanoid": "3.1.10",
    "nestjs-redis": "1.2.8",
    "nestjs-typegoose": "7.1.28",
    "nodemailer": "6.4.10",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "picgo": "1.4.11",
    "pluralize": "8.0.0",
    "reflect-metadata": "0.1.13",
    "ua-parser-js": "0.7.21"
  },
  "devDependencies": {
    "@nestjs/cli": "7.4.0",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.3.2",
    "@types/bcrypt": "3.0.0",
    "@types/ejs": "3.0.4",
    "@types/jest": "26.0.7",
    "@types/lodash": "4.14.158",
    "@types/mongoose": "5.7.21",
    "@types/nanoid": "2.1.0",
    "@types/node": "12.12.47",
    "@types/nodemailer": "6.4.0",
    "@types/pluralize": "0.0.29",
    "@types/socket.io": "2.1.10",
    "@types/supertest": "2.0.10",
    "@types/ua-parser-js": "0.7.33",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "cross-env": "7.0.2",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "pino-colada": "2.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rxjs": "6.6.0",
    "start-server-webpack-plugin": "2.2.5",
    "supertest": "4.0.2",
    "ts-jest": "26.1.3",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "2.5.0"
  },
  "jest": {
    "rootDir": "./",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/db": "<rootDir>/libs/db/src",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src"
    },
    "moduleDirectories": [
      ".",
      "node_modules",
      "src"
    ]
  }
}
