{
  "name": "@mx-space/server",
  "version": "0.11.1",
  "description": "Mix Space Server",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mx-space/server.git"
  },
  "author": "Innei",
  "bugs": {
    "url": "https://github.com/mx-space/server/issues"
  },
  "files": [
    "dist/src/*",
    "dist/libs/*"
  ],
  "homepage": "https://github.com/mx-space/server#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start -w | pino-colada",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "prod": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js",
    "prod:pm2": "cross-env NODE_ENV=production pm2 restart ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:debug": "cross-env NODE_ENV=production nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.13",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-fastify": "^7.1.0",
    "@nestjs/platform-socket.io": "^7.1.0",
    "@nestjs/platform-ws": "^7.1.0",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/swagger": "^4.5.8",
    "@nestjs/websockets": "^7.1.0",
    "@typegoose/auto-increment": "^0.4.1",
    "@typegoose/typegoose": "^7.1.2",
    "akismet": "^2.0.5",
    "bcrypt": "^4.0.1",
    "cache-manager": "^3.3.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cos-nodejs-sdk-v5": "^2.5.20",
    "dayjs": "^1.8.26",
    "ejs": "^3.1.3",
    "fastify": "^2.14.0",
    "fastify-cookie": "^3.6.1",
    "fastify-multipart": "^1.0.5",
    "fastify-session": "^4.0.0",
    "fastify-swagger": "^2.5.1",
    "file-type": "^14.4.0",
    "get-folder-size": "^2.0.1",
    "image-size": "^0.8.3",
    "mongoose": "^5.9.15",
    "mongoose-lean-virtuals": "^0.6.3",
    "mongoose-unique-validator": "^2.0.3",
    "nanoid": "^3.1.9",
    "nestjs-redis": "^1.2.8",
    "nestjs-typegoose": "^7.1.24",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "picgo": "^1.4.8",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/ejs": "^3.0.4",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.152",
    "@types/mongoose": "^5.7.21",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^12",
    "@types/nodemailer": "^6.4.0",
    "@types/pluralize": "^0.0.29",
    "@types/socket.io": "^2.1.6",
    "@types/supertest": "^2.0.9",
    "@types/ua-parser-js": "^0.7.33",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "pino-colada": "^2.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "rootDir": "./",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/db/(.*)": "<rootDir>/libs/db/src/$1",
      "@libs/db": "<rootDir>/libs/db/src",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src"
    }
  }
}
